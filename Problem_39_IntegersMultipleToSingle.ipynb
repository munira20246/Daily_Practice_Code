   ],
   "source": [
    "def intTostr(x):\n",
    "    stri =''.join(map(str, x))\n",
    "    inti = int(stri)\n",
    "    return inti, type(inti)\n",
    "\n",
    "a = [23, 83, 22]\n",
    "intTostr(a)\n"
   ]
  }
